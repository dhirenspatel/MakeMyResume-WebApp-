                <button type="button" class="btn btn-primary hideskillbtn" id="btnSkillSaveAndNew">Save and Add New</button>
                <span class="load-for-skill btn-load-process"><img src="@Url.Content("/Content/images/ajax-load.gif")" /></span>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

