    $("#pusername").keyup(function () {
        $(".err-username").hide();
        $("#pusername").css("border", "1px solid #cccccc");
        var term = $(this).val().trim();
        if (term != "") {
            $.getJSON("/Profile/CheckForUsername", { term: term },
                function (data) {
                    if (data) {
                        $(".err-username").fadeIn();
                        $("#pusername").css("border", "1px solid red");
                        flag2err_username = false;
                    }
                    else {
                        flag2err_username = true;
                    }
                });
        }
        else {
            flag2err_username = false;
        }
    });
    $("#btnUsername").click(function () {
        
        if (!flag2err_username) {
            return false;
        }
    });

    $(".uploadPic").change(function () {
        var fileName = new String();
        fileName = $(this).val();

        if (!validExtension(fileName)) {
            $(this).css("border", "1px solid red");
            $("#btnUploadPic").prop("disabled", true);
        }
        else {
            $(this).css("border", "none");
            $("#btnUploadPic").prop("disabled", false);
        }

    });

    function validExtension(fileName) {

        var fileExtension = new String();
        fileExtension = fileName.substring(fileName.lastIndexOf('.') + 1);

        var validFileExtensions = new Array("jpg", "png", "gif", "bmp");

        var flag = false;
        var size = ($(this).size) / 1024;

        for (var index = 0; index < validFileExtensions.length; index++) {
            if (fileExtension.toLowerCase() == validFileExtensions[index].toString().toLowerCase()) {
                flag = true;
            }
        }
        return flag;
    }
    
    $("#editSocialLinks").click(function () {
        $("#FacebookLink").val("");
        $("#GooglePlusLink").val("");
        $("#LinkedInLink").val("");
        $("#TwitterLink").val("");

        $.getJSON("/Profile/FetchSocialLinks", null,
            function (data) {
                $("#FacebookLink").val(data.FacebookLink);
                $("#GooglePlusLink").val(data.GooglePlusLink);
                $("#LinkedInLink").val(data.LinkedInLink);
                $("#TwitterLink").val(data.TwitterLink);
            });
    });

    $("#btnSocialLink").click(function () {
        var FacebookLink = $("#FacebookLink").val().trim();
        var GoogleLink = $("#GooglePlusLink").val().trim();
        var LinkedInLink = $("#LinkedInLink").val().trim();
        var TwitterLink = $("#TwitterLink").val().trim();
        
        $.getJSON("/Profile/UpdateSocialLinks", { FacebookLink: FacebookLink, GoogleLink: GoogleLink, LinkedInLink: LinkedInLink, TwitterLink: TwitterLink },
            function (data) {
                $("#SocialLinks").modal("hide");
                $("#lnkFacebok").attr("href", "http://" + FacebookLink);
                $("#lnkGoogle").attr("href", "http://" + GoogleLink);
                $("#lnkLinkedIn").attr("href", "http://" + LinkedInLink);
                $("#lnkTwitter").attr("href", "http://" + TwitterLink);
            });
    });

   



</script>



