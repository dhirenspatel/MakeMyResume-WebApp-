                        </span>
                    </div>
                </div>
            </div>
            <div id="collapseEducation" class="panel-collapse collapse">
                <div class="panel-body">
                    <div class="row appendEducation">
                        @if (Model.Educations != null)
                        {
                            foreach (var a in @Model.Educations)
                            {
                            <div id="education-@a.Id">
                                <div class="col-md-12">
                                    <h4 class="text-primary">@a.Degree <small>(@a.FromYear - @a.ToYear)</small>
                                        <span style="cursor:pointer" class="deleteEducation pull-right" id="@a.Id"><i class="glyphicon glyphicon-remove-circle"></i></span>
                                        <span style="cursor:pointer;margin-right:7px" class="editEducation pull-right" id="@a.Id"><i class="glyphicon glyphicon-edit"></i></span>
                                    </h4>
                                    <small>@a.College</small>
                                </div>
                                <hr />
                            </div>
                            }
                        }
