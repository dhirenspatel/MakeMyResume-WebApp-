@model meraRESU.ME.Models.Employee
@{
    ViewBag.Title = "Resume";
    string fileName = string.Empty;
    string extension = string.Empty;
    string profileImage = string.Empty;
}

<div id="navbar" class="navbar navbar-default">
    <div class="fixed-container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <div class="raw">
                <a class="navbar-brand" href="/">
                    <img src="~/Content/images/logo.png" style="height: 30px;" />
                    | <small style="color: #d35400">Save Papers, Save Bandwidth</small> </a>
            </div>
        </div>

        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav pull-right">
                <li><a href="/Home/Logout"><i class="glyphicon glyphicon-off"></i> Logout</a></li>
            </ul>
        </div>

    </div>
    @if (Model != null)
    {
        if (Model.UserName != null)
        {

            if (Model.UserName != "")
            {
        <div class="col-lg-2"></div>
        <div class="col-lg-7 text-center">
            @{ string u = "/@" + Model.UserName;
               string url = System.Web.Configuration.WebConfigurationManager.AppSettings["DomainName"] + "/@" + Model.UserName;}
            Your Resume Link : <a target="_blank" href="@u">@url</a>
        </div>
        <div class="col-lg-3">
            @if (Model.FacebookLink != null)
            {
                <a target="_blank" href="http://@Model.FacebookLink" id="lnkFacebok">
                    <img src="~/Content/images/facebook-icon.png" /></a>
            }
            else
            {
                <a target="_blank" href="#" id="lnkFacebok">
                    <img src="~/Content/images/facebook-icon.png" /></a>
            }
            @if (Model.GooglePlusLink != null)
            {
               
                <a target="_blank" href="http://@Model.GooglePlusLink" id="lnkGoogle">
                    <img src="~/Content/images/Google-plus-icon.png" /></a>
               
            }
            else
            {
                <a target="_blank" href="#" id="lnkGoogle">
                    <img src="~/Content/images/Google-plus-icon.png" /></a>
            }

            @if (Model.LinkedInLink != null)
            {
                
                <a target="_blank" href="http://@Model.LinkedInLink" id="lnkLinkedIn">
                    <img src="~/Content/images/Linkedin-icon.png" /></a>
               
            }
            else
            {
                <a target="_blank" href="#" id="lnkLinkedIn">
                    <img src="~/Content/images/Linkedin-icon.png" /></a>
            }
            @if (Model.TwitterLink != null)
            {
                <a target="_blank" href="http://@Model.TwitterLink" id="lnkTwitter">
                    <img src="~/Content/images/Twitter-icon.png" /></a>
            }
            else
            {
                <a target="_blank" href="#" id="lnkTwitter">
                    <img src="~/Content/images/Twitter-icon.png" /></a>
            }


            <span id="editSocialLinks" style="cursor: pointer" title="Edit Social Links" class="pull-right" data-toggle="modal" data-target="#SocialLinks">
                <i class="glyphicon glyphicon-edit"></i>
            </span>
        </div>
                      }
                  }
              }
</div>

<div class="outer">
    <div class="">
        <div class="innerOuter">
            <div class="col-md-7">
                <div class="col-md-3">
                    @if (Model.Logo != null)
                    {
                        fileName = Path.GetFileNameWithoutExtension(Model.Logo);
                        extension = Path.GetExtension(Model.Logo);
                        profileImage = fileName + "_81x81" + extension;
                        
                        <img class="thumbnail" src="/Document/@Model.Id/@profileImage" />
                    }
                    else
                    {
                        <img class="thumbnail" src="~/Content/images/thumb.jpg" />
                    }
                </div>
                <div class="col-md-9">

                    <h2 class="text-primary" id="pName" style="margin-top: 0">@Model.FirstName @Model.LastName</h2>
                    <h4 class="text-warning">@Model.Designation</h4>

                    <p class="text-muted">
                        Work Exp.: 
                        <b class="totWorkExp">
                            @if (@Model.WorkExYear != null)
                            {
                                @Model.WorkExYear <text>year</text> 
                            }
                            @if (@Model.WorkExMonth != null)
                            {
                                @Model.WorkExMonth <text>month</text> 
                            }

                        </b>
                        <span style="cursor:pointer;margin-right:7px" class="editWorkExp pull-right" id="@Model.Id"><i class="glyphicon glyphicon-edit"></i></span>
                    </p>
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-12">
                    <a class="btn btn-xs btn-success" data-toggle="modal" href="#modelUploadPhoto">Upload Photo</a>
                </div>
                
                
            </div>
            <div class="col-md-5">
                <span style="cursor:pointer;margin-right:7px" class="editProfile pull-right" id="@Model.Id"><i class="glyphicon glyphicon-edit"></i></span>
                <dl class="dl-horizontal text-muted">
                    <dt class="dt-custome">Address:</dt>
                    <dd class="dd-custome" id="paddr">@Model.Address</dd>

                    <dt class="dt-custome">Phone:</dt>
                    <dd class="dd-custome" id="pphone">@Model.Phone</dd>

                    <dt class="dt-custome">Email:</dt>
                    <dd class="dd-custome" id="pemail">@Model.Email</dd>

                    <dt class="dt-custome">Date of Birth:</dt>
                    <dd class="dd-custome" id="pdob">
                        @if (@Model.DateOfBirth != null)
                        {
                            @Model.DateOfBirth.Value.ToString("dd-MM-yyyy")
                        }
                    </dd>
                    @if (Model.Gender != null)
                    {
                        <dt class="dt-custome">Gender:</dt>
                        <dd class="dd-custome" id="pgender" style="text-transform: capitalize">@Model.Gender</dd>
                    }
                    <dt class="dt-custome">Nationality:</dt>
                    <dd class="dd-custome" id="pnat">@Model.Nationality</dd>


                </dl>

            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-10 text-primary"><b>Objective:</b></div>
        <div class="col-lg-2"><span style="cursor:pointer;margin-right:23px" class="editObjective pull-right" id="@Model.Id"><i class="glyphicon glyphicon-edit"></i></span></div>
        
    </div>
    <div class="row">
        <div class="col-md-12 myObj" style="word-break:break-all;text-align:justify">@Model.Objective</div>
        </div>
    <hr />
    <div class="panel-group" id="myCollapsible">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-lg-11">
                        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapseSkills">
                            <h4 class="panel-title"><span class="glyphicon glyphicon-minus"></span>
                                Skill and Expertise 
                
                            </h4>

                        </a>
                    </div>
                    <div class="col-lg-1">
                        <span style="cursor: pointer" id="addSkill" class="pull-right" data-toggle="modal" data-target="#skill">
                            <i class="glyphicon glyphicon-plus-sign"></i>
                        </span>
                    </div>
                </div>

            </div>
            <div id="collapseSkills" class="panel-collapse collapse in">
                <div class="panel-body" style="word-break: break-all">
                    <ul class="appendSkill">
                        @if (Model.EmployeeSkills != null)
                        {
                            foreach (var a in @Model.EmployeeSkills)
                            {
                            <li id="skill-@a.Id">@a.Skill
                                <span style="cursor:pointer" class="deleteSkill pull-right" id="@a.Id"><i class="glyphicon glyphicon-remove-circle"></i></span>
                                <span style="cursor:pointer;margin-right:7px" class="editSkill pull-right" id="@a.Id"><i class="glyphicon glyphicon-edit"></i></span>
                            </li>
                            }
                        }
                    </ul>

                </div>
            </div>
        </div>

        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-lg-11">
