                    <div class="col-md-12 appendProject">
                        @if (Model.EmployeeProjects != null)
                        {
                            foreach (var a in @Model.EmployeeProjects)
                            {
                            <div id="project-@a.Id">
                                <h4 class="text-primary">@a.Title
                                    <span style="cursor:pointer" class="deleteProject pull-right" id="@a.Id"><i class="glyphicon glyphicon-remove-circle"></i></span>
                                    <span style="cursor:pointer;margin-right:7px" class="editProject pull-right" id="@a.Id"><i class="glyphicon glyphicon-edit"></i></span>
                                </h4>
                                <p>@Html.Raw(a.Description)</p>
                                <hr />
                            </div>
                            }
                        }
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-default">
            <div class="panel-heading">
