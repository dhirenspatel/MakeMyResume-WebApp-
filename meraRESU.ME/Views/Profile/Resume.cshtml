<script src="~/Scripts/jquery-ui-1.8.24.min.js"></script>
<link href="~/Content/css/ui.datepicker.css" rel="stylesheet" />

<link href="http://netdna.bootstrapcdn.com/font-awesome/3.0.2/css/font-awesome.css" rel="stylesheet">
<script src="~/Content/js/bootstrap-wysiwyg.js"></script>
<script src="~/Content/js/jquery.hotkeys.js"></script>

<script type="text/javascript">

    $("#Description3Div").wysiwyg({ toolbarSelector: '[data-role=editor-toolbar]' });
    $("#Description4Div").wysiwyg({ toolbarSelector: '[data-role=editor1-toolbar]' });
    $("#Description5Div").wysiwyg({ toolbarSelector: '[data-role=editor2-toolbar]' });
    $("#Description6Div").wysiwyg({ toolbarSelector: '[data-role=editor3-toolbar]' });

    
    $("#Description3Div").bind("keyup click", function () {
        var Description3 = $(this).html();
        $("#Description3").val(Description3);
    });
    $("#Description4Div").bind("keyup click", function () {
        var Description4 = $(this).html();
        $("#Description4").val(Description4);
    });
    $("#Description5Div").bind("keyup click", function () {
        var Description5 = $(this).html();
        $("#Description5").val(Description5);
    });
    $("#Description6Div").bind("keyup click", function () {
        var Description6 = $(this).html();
        $("#Description6").val(Description6);
    });

    $("#ProfDOB").datepicker({ dateFormat: 'dd-mm-yy', changeMonth: true, changeYear: true, yearRange: '1905:@DateTime.Today.Year' });

    $('.modal').attr("data-backdrop", "static");
    $('#profile').attr("data-keyboard", false);

    $(document).ready(function () {
        $("#ui-datepicker-div").hide();
        var flag2username = '@Session["nousername"]';
        if (flag2username == "True") {
            $('#getusername').attr("data-backdrop", "static");
            $('#getusername').attr("data-keyboard", false);
            $('#getusername').modal('toggle');
        }

