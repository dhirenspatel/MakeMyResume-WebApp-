                                <span style="cursor:pointer" class="deletePublication pull-right" id="@a.Id"><i class="glyphicon glyphicon-remove-circle"></i></span>
                                <span style="cursor:pointer;margin-right:7px" class="editPublication pull-right" id="@a.Id"><i class="glyphicon glyphicon-edit"></i></span>
                            </li>
                            }
